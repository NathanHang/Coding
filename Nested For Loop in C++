#include <iostream>

using namespace std;

int main()
{
    for(int i = 1; i <= 5; i++){
        cout << "row " << i << ": ";
        for(int j = 1; j <= 5; j++){
            cout << "column " << j;
            if(j < 5){
                cout << ", ";
            }    
        }
        cout << endl;
    }

    return 0;
}
