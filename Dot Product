#include <iostream>
#include <vector>

using namespace std;

int main()
{
    vector<vector<int>> matrix
    {
        {1, 1, 1},
        {2, 2, 2},
        {3, 3, 3},
    };
    int pi[] = {5, 5, 5, 5, 5};
    
    for(int i = 0; i < matrix.size(); i++){
        for(int j = 0; j < matrix[i].size(); j++){
            cout << matrix[i][j]*pi[i] << " ";
        }
    }

    return 0;
}
